@model List<OurPlace.Models.User.SearchUserModel>
@{
    ViewData["Title"] = "Search";
}

<div class="container">
    <h3>Search result</h3>
    <hr />
    <br />
    <table class="table table-striped">
        <thead>
            <tr>
                <th>First name</th>
                <th>Last name</th>
                <th>Email adress</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                    <tr>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.Email</td>
                        <td><a asp-action="Profile" asp-route-userId="@user.Id">Visit profile</a></td>
                    </tr>
                
            }
        </tbody>
    </table>
</div>

